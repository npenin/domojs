{
    "name": "domojs",
    "repository": "https://github.com/npenin/domojs",
    "author": "npenin <nicolas.penin@dragon-angel.fr>",
    "version": "141.0.0",
    "license": "MIT",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "devDependencies": {
        "@akala/automate": "^6.8.85",
        "@akala/automate-yamlloader": "^3.1.85",
        "@akala/cli": "^5.16.8",
        "@akala/commands": "^18.1.15",
        "@akala/pm": "^15.11.39",
        "@akala/semantic-release": "^3.2.65",
        "typescript": "^5.9.3"
    },
    "scripts": {
        "publish-all": "yarn akala-automate-runner --loader @akala/automate-yamlloader/ --file @akala/semantic-release/publish.yml --verbose=silly",
        "ci-deps": "git commit -m 'chore(deps): update dependencies'",
        "generate": "yarn workspaces foreach -A run generate",
        "generate-metadata": "yarn workspaces foreach -A run generate-metadata",
        "start": "yarn workspace @domojs/web vite src",
        "docker:build": "docker build -t domojs-dev -f packages/docker/Dockerfile.dev .",
        "docker:dev": "docker-compose up -d domojs-dev",
        "docker:test": "docker-compose up domojs-test --build",
        "docker:down": "docker-compose down",
        "docker:rebuild": "docker-compose build --no-cache && docker-compose up -d",
        "docker:logs": "docker-compose logs -f",
        "docker:shell": "docker exec -it domojs-dev sh"
    },
    "packageManager": "yarn@4.12.0"
}
