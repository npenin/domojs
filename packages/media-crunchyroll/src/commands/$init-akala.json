{
    "$schema": "https://raw.githubusercontent.com/npenin/akala/master/packages/commands/command-schema.json",
    "fs": {
        "inject": [
            "$container",
            "param.0"
        ]
    },
    "cli": {
        "inject": [
            "$container",
            "context.state"
        ]
    },
    "doc": {
        "description": "Not meant to be invoked directly. Initializes the configuration when another command is executed via the akala CLI",
        "inject": [
            "$container",
            "The root akala configuration"
        ]
    },
    "": {
        "inject": [
            "$container",
            "param.0"
        ]
    }
}