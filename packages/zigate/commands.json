{
    "name": "@domojs/zigate",
    "commands": [
        {
            "name": "$init",
            "config": {
                "fs": {
                    "path": "dist/commands/$init.js",
                    "source": "src/commands/$init.ts",
                    "inject": [
                        "params.0",
                        "params.1",
                        "params.2"
                    ]
                },
                "cli": {
                    "inject": [
                        "context",
                        "$container",
                        "signal"
                    ]
                },
                "": {
                    "inject": [
                        "params.0",
                        "params.1",
                        "params.2"
                    ]
                }
            }
        }
    ],
    "dependencies": [
        "@domojs/devices"
    ],
    "$schema": "https://raw.githubusercontent.com/npenin/akala/main/packages/commands/container-schema.json"
}