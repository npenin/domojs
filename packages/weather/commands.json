{
    "name": "@domojs/weather",
    "commands": [
        {
            "name": "$init",
            "config": {
                "fs": {
                    "path": "dist/commands/$init.js",
                    "source": "src/commands/$init.ts",
                    "inject": [
                        "container",
                        "options.path"
                    ]
                },
                "cli": {
                    "inject": [
                        "context"
                    ]
                },
                "": {
                    "inject": [
                        "container",
                        "options.path"
                    ]
                }
            }
        },
        {
            "name": "temperature",
            "config": {
                "fs": {
                    "inject": [
                        "$http",
                        "params.0",
                        "params.1"
                    ],
                    "path": "dist/commands/temperature.js",
                    "source": "src/commands/temperature.ts"
                },
                "jsonrpc": {
                    "inject": [
                        "$http",
                        "params.0",
                        "params.1"
                    ]
                },
                "": {
                    "inject": [
                        "$http",
                        "params.0",
                        "params.1"
                    ]
                },
                "http": {
                    "route": "/temperature",
                    "method": "get",
                    "inject": [
                        "$http",
                        "query.lat",
                        "query.long"
                    ]
                },
                "cli": {
                    "inject": [
                        "$http",
                        "options.latitude",
                        "options.longitude"
                    ]
                }
            }
        },
        {
            "name": "today",
            "config": {
                "fs": {
                    "inject": [
                        "$http",
                        "params.0",
                        "params.1"
                    ],
                    "path": "dist/commands/today.js",
                    "source": "src/commands/today.ts"
                },
                "jsonrpc": {
                    "inject": [
                        "$http",
                        "params.0",
                        "params.1"
                    ]
                },
                "": {
                    "inject": [
                        "$http",
                        "params.0",
                        "params.1"
                    ]
                },
                "http": {
                    "route": "/today",
                    "method": "get",
                    "inject": [
                        "$http",
                        "query.lat",
                        "query.long"
                    ]
                },
                "cli": {
                    "inject": [
                        "$http",
                        "options.latitude",
                        "options.longitude"
                    ]
                }
            }
        }
    ],
    "$schema": "https://raw.githubusercontent.com/npenin/akala/main/packages/commands/container-schema.json"
}