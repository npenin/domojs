{
    "name": "library",
    "commands": [
        {
            "name": "$init",
            "inject": [],
            "config": {
                "fs": {
                    "inject": [
                        "param.0",
                        "param.1"
                    ],
                    "path": "dist/server/commands/library/$init.js",
                    "source": "src/server/commands/library/$init.ts"
                },
                "cli": {
                    "inject": [
                        "context",
                        "options.configPath"
                    ],
                    "options": {
                        "configPath": {
                            "normalize": true,
                            "doc": "Path to the configuration file",
                            "needsValue": true
                        }
                    }
                },
                "": {
                    "inject": []
                }
            }
        },
        {
            "name": "browse",
            "inject": [],
            "config": {
                "fs": {
                    "path": "dist/server/commands/library/browse.js",
                    "source": "src/server/commands/library/browse.ts",
                    "inject": [
                        "param.0",
                        "param.1",
                        "param.2",
                        "param.3",
                        "param.4",
                        "param.5",
                        "param.6",
                        "param.7"
                    ]
                },
                "": {
                    "inject": []
                }
            }
        },
        {
            "name": "dropbox",
            "inject": [],
            "config": {
                "fs": {
                    "path": "dist/server/commands/library/dropbox.js",
                    "source": "src/server/commands/library/dropbox.ts",
                    "inject": [
                        "param.0",
                        "param.1",
                        "param.2",
                        "param.3",
                        "param.4",
                        "param.5",
                        "param.6",
                        "param.7"
                    ]
                },
                "": {
                    "inject": []
                }
            }
        },
        {
            "name": "register-scrapper",
            "inject": [],
            "config": {
                "fs": {
                    "path": "dist/server/commands/library/register-scrapper.js",
                    "source": "src/server/commands/library/register-scrapper.ts",
                    "inject": [
                        "param.0",
                        "param.1"
                    ]
                },
                "": {
                    "inject": []
                },
                "jsonrpc": {
                    "inject": [
                        "$container",
                        "$connectionAsContainer",
                        "param.0",
                        "param.1"
                    ]
                }
            }
        },
        {
            "name": "scrap",
            "inject": [],
            "config": {
                "fs": {
                    "path": "dist/server/commands/library/scrap.js",
                    "source": "src/server/commands/library/scrap.ts",
                    "inject": [
                        "param.0"
                    ]
                },
                "": {
                    "inject": []
                },
                "cli": {
                    "inject": [
                        "$container",
                        "param.0"
                    ]
                }
            }
        }
    ],
    "$schema": "https://raw.githubusercontent.com/npenin/akala-commands/master/schema.json"
}