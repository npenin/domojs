{
    "name": "@domojs/media",
    "version": "1.5.5",
    "description": "",
    "main": "dist/server/index.js",
    "types": "dist/server/index.d.ts",
    "scripts": {
        "test": "echo \"Error: no test specified\"",
        "build:js": "tsc -p .",
        "build:js:routes": "browserify -x @akala/client -x @akala/core -x @akala/json-rpc-ws dist/client/routes.js -o dist/routes.js",
        "build:js:tile": "browserify -x @akala/client -x @akala/core -i @domojs/theme-default/dist/tile dist/client/tile.js -o dist/tile.js",
        "build": "npm run build:js && npm run build:js:routes && npm run build:js:tile",
        "generate": "yarn generate-library",
        "generate-metadata": "yarn generate-metadata-library",
        "generate-library": "ac generate dist/server/commands/library library.json --name @domojs/media",
        "generate-metadata-library": "ac generate-metadata library.json library.ts --name @domojs/media"
    },
    "author": "npenin",
    "license": "MIT",
    "dependencies": {
        "@akala/client": "^5.3.7",
        "@akala/commands": "^3.8.0",
        "@akala/config": "^1.3.3",
        "@akala/core": "^9.2.1",
        "levenshtein": "1.0.5"
    },
    "devDependencies": {
        "@akala/cli": "^2.3.4",
        "@akala/json-rpc-ws": "^9.6.0",
        "@akala/server": "^8.3.8",
        "@akala/storage": "^3.2.2",
        "@domojs/theme-default": "workspace:*",
        "@types/debug": "4.1.7",
        "@types/ioredis": "4.28.10",
        "@types/jquery": "3.5.14",
        "@types/levenshtein": "1.0.1",
        "@types/redis": "4.0.11",
        "@types/request": "2.48.8",
        "@types/ws": "8.5.3",
        "typescript": "4.8.3"
    }
}
