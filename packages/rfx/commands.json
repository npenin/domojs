{
    "name": "commands",
    "commands": [
        {
            "name": "$init",
            "config": {
                "fs": {
                    "path": "dist/server/commands/$init.js",
                    "source": "src/server/commands/$init.ts",
                    "inject": []
                },
                "cli": {
                    "inject": [
                        "$container"
                    ]
                },
                "": {
                    "inject": []
                }
            }
        },
        {
            "name": "exec",
            "config": {
                "fs": {
                    "path": "dist/server/commands/exec.js",
                    "source": "src/server/commands/exec.ts",
                    "inject": [
                        "param.0",
                        "param.1",
                        "param.2"
                    ]
                },
                "": {
                    "inject": [
                        "param.0",
                        "param.1",
                        "param.2"
                    ]
                },
                "cli": {
                    "usage": "exec <device> <command> [value]",
                    "inject": [
                        "options.device",
                        "options.command",
                        "param.0"
                    ]
                }
            }
        },
        {
            "name": "rfy",
            "config": {
                "fs": {
                    "path": "dist/server/commands/rfy.js",
                    "source": "src/server/commands/rfy.ts",
                    "inject": [
                        "param.0",
                        "param.1",
                        "param.2",
                        "param.3",
                        "param.4"
                    ]
                },
                "jsonrpc": {
                    "inject": [
                        "param.0",
                        "param.1",
                        "param.2",
                        "param.3",
                        "param.4"
                    ]
                },
                "": {
                    "inject": [
                        "param.0",
                        "param.1",
                        "param.2",
                        "param.3",
                        "param.4"
                    ]
                },
                "cli": {
                    "inject": [
                        "options.unitCode",
                        "options.id1",
                        "options.id2",
                        "options.id3",
                        "param.0"
                    ]
                }
            }
        },
        {
            "name": "save",
            "config": {
                "fs": {
                    "inject": [
                        "param.0",
                        "param.1",
                        "connectionAsContainer"
                    ],
                    "path": "dist/server/commands/save.js",
                    "source": "src/server/commands/save.ts"
                },
                "jsonrpc": {
                    "inject": [
                        "param.0",
                        "param.1",
                        "connectionAsContainer"
                    ]
                },
                "": {
                    "inject": []
                }
            }
        }
    ],
    "$schema": "https://raw.githubusercontent.com/npenin/akala/master/packages/commands/container-schema.json"
}