{
    "name": "commands",
    "commands": [
        {
            "name": "$init",
            "config": {
                "fs": {
                    "path": "dist/server/commands/$init.js",
                    "source": "src/server/commands/$init.ts",
                    "inject": [
                        "params.0",
                        "params.1"
                    ]
                },
                "cli": {
                    "inject": [
                        "context",
                        "signal"
                    ]
                },
                "": {
                    "inject": [
                        "params.0",
                        "params.1"
                    ]
                }
            }
        }
    ],
    "$schema": "https://raw.githubusercontent.com/npenin/akala/main/packages/commands/container-schema.json"
}