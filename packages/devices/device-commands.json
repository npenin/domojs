{
    "name": "devices",
    "commands": [
        {
            "name": "$init",
            "config": {
                "fs": {
                    "path": "dist/server/commands/devices/$init.js",
                    "source": "src/server/commands/devices/$init.ts",
                    "inject": [
                        "params.0",
                        "params.1",
                        "params.2"
                    ]
                },
                "cli": {
                    "inject": [
                        "context",
                        "pm",
                        "$container"
                    ]
                },
                "": {
                    "inject": [
                        "params.0",
                        "params.1",
                        "params.2"
                    ]
                }
            }
        },
        {
            "name": "register-adapter",
            "config": {
                "fs": {
                    "inject": [
                        "params.0",
                        "params.1"
                    ],
                    "path": "dist/server/commands/devices/register-adapter.js",
                    "source": "src/server/commands/devices/register-adapter.ts"
                },
                "": {
                    "inject": [
                        "params.0",
                        "params.1"
                    ]
                },
                "mqtt": {
                    "topics": [
                        "domojs/devices"
                    ],
                    "inject": [
                        "json"
                    ]
                }
            }
        },
        {
            "name": "request-permissions",
            "config": {
                "fs": {
                    "path": "dist/server/commands/devices/request-permissions.js",
                    "source": "src/server/commands/devices/request-permissions.ts",
                    "inject": [
                        "params.0",
                        "params.1",
                        "params.2"
                    ]
                },
                "": {
                    "inject": [
                        "params.0",
                        "params.1",
                        "params.2"
                    ]
                }
            }
        }
    ],
    "$schema": "https://raw.githubusercontent.com/npenin/akala/main/packages/commands/container-schema.json"
}