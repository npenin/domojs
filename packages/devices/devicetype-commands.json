{
    "name": "device-types",
    "commands": [
        {
            "name": "$init",
            "inject": [],
            "config": {
                "": {
                    "inject": []
                },
                "fs": {
                    "path": "dist/server/commands/device-types/$init.js",
                    "source": "src/server/commands/device-types/$init.ts",
                    "inject": []
                },
                "cli": {
                    "options": {
                        "normalize": [
                            "path"
                        ]
                    },
                    "inject": [
                        "$container",
                        "pm",
                        "options.path",
                        "options"
                    ]
                }
            }
        },
        {
            "name": "list",
            "inject": [],
            "config": {
                "": {
                    "inject": []
                },
                "fs": {
                    "path": "dist/server/commands/device-types/list.js",
                    "source": "src/server/commands/device-types/list.ts",
                    "inject": []
                },
                "http": {
                    "inject": [],
                    "method": "get",
                    "route": "/list"
                }
            }
        },
        {
            "name": "push-status",
            "inject": [
                "param.0"
            ],
            "config": {
                "": {
                    "inject": [
                        "param.0"
                    ]
                },
                "fs": {
                    "path": "dist/server/commands/device-types/push-status.js",
                    "source": "src/server/commands/device-types/push-status.ts",
                    "inject": [
                        "param.0"
                    ]
                }
            }
        },
        {
            "name": "register",
            "inject": [
                "param.0",
                "$container",
                "connectionAsContainer"
            ],
            "config": {
                "": {
                    "inject": [
                        "param.0",
                        "$container",
                        "connectionAsContainer"
                    ]
                },
                "fs": {
                    "path": "dist/server/commands/device-types/register.js",
                    "source": "src/server/commands/device-types/register.ts",
                    "inject": [
                        "param.0",
                        "$container",
                        "connectionAsContainer"
                    ]
                },
                "jsonrpc": {
                    "inject": [
                        "param.0",
                        "$container",
                        "connectionAsContainer"
                    ]
                }
            }
        }
    ],
    "$schema": "https://raw.githubusercontent.com/npenin/akala-commands/master/schema.json"
}