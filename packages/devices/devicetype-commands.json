{
    "name": "device-types",
    "commands": [
        {
            "name": "$init",
            "inject": [],
            "config": {
                "fs": {
                    "path": "dist/server/commands/device-types/$init.js",
                    "source": "src/server/commands/device-types/$init.ts",
                    "inject": []
                },
                "cli": {
                    "inject": [
                        "context",
                        "$container",
                        "pm"
                    ]
                },
                "": {
                    "inject": []
                }
            }
        },
        {
            "name": "add",
            "inject": [],
            "config": {
                "fs": {
                    "inject": [
                        "$container",
                        "param.0",
                        "param.1"
                    ],
                    "path": "dist/server/commands/device-types/add.js",
                    "source": "src/server/commands/device-types/add.ts"
                },
                "": {
                    "inject": [
                        "$container",
                        "param.0",
                        "param.1"
                    ]
                },
                "jsonrpc": {
                    "inject": [
                        "$container",
                        "param.0",
                        "param.1"
                    ]
                },
                "cli": {
                    "inject": [
                        "$container",
                        "param.0",
                        "stdin"
                    ]
                },
                "http": {
                    "inject": [
                        "$container",
                        "route.type",
                        "body"
                    ],
                    "type": "json",
                    "method": "post",
                    "route": "/new/:type"
                }
            }
        },
        {
            "name": "list",
            "inject": [],
            "config": {
                "fs": {
                    "inject": [],
                    "path": "dist/server/commands/device-types/list.js",
                    "source": "src/server/commands/device-types/list.ts"
                },
                "": {
                    "inject": []
                },
                "cli": {
                    "inject": []
                },
                "http": {
                    "inject": [],
                    "method": "get",
                    "route": "/list"
                }
            }
        },
        {
            "name": "push-status",
            "inject": [],
            "config": {
                "fs": {
                    "path": "dist/server/commands/device-types/push-status.js",
                    "source": "src/server/commands/device-types/push-status.ts",
                    "inject": [
                        "param.0"
                    ]
                },
                "": {
                    "inject": []
                }
            }
        },
        {
            "name": "register",
            "inject": [],
            "config": {
                "fs": {
                    "inject": [
                        "param.0",
                        "$container",
                        "connectionAsContainer",
                        "connection"
                    ],
                    "path": "dist/server/commands/device-types/register.js",
                    "source": "src/server/commands/device-types/register.ts"
                },
                "jsonrpc": {
                    "inject": [
                        "param.0",
                        "$container",
                        "connectionAsContainer",
                        "connection"
                    ]
