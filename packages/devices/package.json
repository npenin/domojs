  "scripts": {
    "test": "echo \"Error: no test specified\"",
    "build:js": "tsc -p .",
    "build:js:routes": "browserify -x @akala/client -x @akala/core dist/client/routes.js -o dist/routes.js",
    "build:js:tile": "browserify -x @akala/client -x @akala/core -i @domojs/theme-default/dist/tile dist/client/tile.js -o dist/tile.js",
    "build": "npm run build:js && npm run build:js:routes && npm run build:js:tile",
    "generate:devices:json": "ac generate dist/server/commands/devices device-commands.json",
    "generate:devicetypes:json": "ac generate dist/server/commands/device-types devicetype-commands.json",
    "generate:devices:ts": "ac generate-metadata devices device-commands.json src/server/device-commands.ts",
    "generate:devicetypes:ts": "ac generate-metadata devicetype devicetype-commands.json src/server/devicetype-commands.ts",
    "generate:json": "npm run generate:devices:json && npm run generate:devicetypes:json",
    "generate:ts": "npm run generate:devices:ts && npm run generate:devicetypes:ts",
    "generate": "npm run generate:json && npm run generate:ts"
  },
  "typings": "dist/index.d.ts",
  "snyk": true,
  "gitHead": "59da3072912bba96ad5617632580194493a3a43a"
